gravityAOB:
  db D9 81 F0 04 00 00

unregistersymbol(gravityAOB)
dealloc(newmem)

{$lua}
if checkJumpKeys ~= nil then
  checkJumpKeys.destroy()
  checkJumpKeys = nil
end

