      <AssemblerScript>[ENABLE]
aobscanmodule(fovAOB,hitman.exe,74 31 F3 0F 10 05 x x x x F3 0F 11)

alloc(camX,2048,"hitman.exe")
label(returnhereX)
label(exitX)
label(originalCodeX)

label(camera)
