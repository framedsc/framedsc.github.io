label(currentMode)
registersymbol(currentMode)

registersymbol(fovAOB)

/***************************************
           Custom Code Section
***************************************/

/*
Filters out non-camera addresses.
Writes only the camera address to symbol.
*/

camX:
cmp [rbx+38],0
jne originalCodeX
// could also check +20 or +24 for 0 if the above fails
mov [camera],rbx
jmp exitX

exitX:
jmp returnhereX

originalCodeX:
movss [rbx+00000090],xmm0
jmp exitX

/*
Toggles writing to the rotation matrix.
Keeps camera in place when alt-tabbed.
*/

lockmouse:
cmp [toggleLock],1
je toggleMode
jmp checkMode


toggleMode:
mov [toggleLock],0
cmp [currentMode],1
je originalCodeLM
jmp lockCode

checkMode:
cmp [currentMode],1
je lockCode
jmp originalCodeLM

lockCode:
mov [currentMode],1
jmp exit_lock

originalCodeLM:
movups [rbx+00000080],xmm0
mov [currentMode],0
jmp exit_lock

exit_lock:
jmp return_lock

/***************************************
           Default Values
***************************************/

///
camera:
dq 0
currentMode:
dd 0
toggleLock:
dd 0
///

/***************************************
           Opcode Overrides
***************************************/

// camera

"hitman.exe"+419037A:
jmp camX
nop
nop
nop
returnhereX:

"hitman.exe"+4190390:
db 90 90 90 90 90 90 90 90

"hitman.exe"+4190388:
db 90 90 90 90 90 90 90 90

// FOV

fovAOB:
db 90 90

fovAOB+22:
db 90 90 90 90 90 90 90 90

// lock mouse

"hitman.exe"+419036D:
jmp lockmouse
nop
nop
return_lock:


/***************************************
           Disable Section
***************************************/

[DISABLE]
dealloc(newmem)
// CAMERA OPCODES
"hitman.exe"+419037A:
movss [rbx+00000090],xmm0
"hitman.exe"+4190390:
movss [rbx+00000094],xmm1
"hitman.exe"+4190388:
movss [rbx+00000098],xmm0
// FOV OPCODES
fovAOB:
je hitman.CAkRegisteredObj::~CAkRegisteredObj+425C6A
movss xmm0,[hitman.Ordinal277+1303614]
fovAOB+22:
movss [rcx+0000017C],xmm0
// LOCK MOUSE
"hitman.exe"+419036D:
movups [rbx+00000080],xmm0
unregistersymbol(camera)
unregistersymbol(fovAOB)

