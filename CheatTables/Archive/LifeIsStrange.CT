{$lua}
if syntaxcheck then return end
local symbolCheckT = createTimer()
symbolCheckT.Interval = 50
symbolCheckT.OnTimer = function(sender)

  local base = readInteger("_settings")
  if base == 0 or base == nil then return end

  local function outbaseAD()
    local outbase = readInteger("_outlines")
    if outbase == 0 or outbase == nil then return 0 end
    return outbase
  end

  -- ADDRESSES
  local overlaysAD  = base - 0xED70
  local aberrationAD  = base - 0xC8B0

  -- FUNCTIONS
  local function toggleAberration()
    if readInteger(aberrationAD) == 0 then
      writeInteger(aberrationAD, 1)
    else writeInteger(aberrationAD, 0)
    end
  end

  local function toggleOverlays()
    if readInteger(overlaysAD) == 0 then
      writeInteger(overlaysAD, 1)
    else writeInteger(overlaysAD, 0)
    end
  end

  local function toggleOutlines()
    local outbase = outbaseAD()
    if outbase ~= 0 then
      local outlinesAD = outbase + 0x4F9C
      outlines = readInteger(outlinesAD)
      if outlines ~= nil then
        if outlines == 1 then
          writeInteger(outlinesAD, 0)
        else writeInteger(outlinesAD, 1)
        end
      end
    end
  end

  -- HOTKEYS
  funcHotkeys = {
    createHotkey(toggleAberration, VK_F2),
    createHotkey(toggleOutlines, VK_F3),
    createHotkey(toggleOverlays, VK_F4)
  }
  for i,v in ipairs(funcHotkeys) do
    v.DelayBetweenActivate = 1000
  end

  sender.destroy()
end

