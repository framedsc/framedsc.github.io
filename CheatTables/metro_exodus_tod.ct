  <LuaScript>-----------------------------------
-- METRO EXODUS TABLE --
-- BY DRAZERAK --
-- GAME VERSION 1.0.1.2/1.0.0.2 --
-----------------------------------
-----------------------------------------------------------------------
-- AUTOMATIC PROCESS SELECTION --
-----------------------------------------------------------------------

function OpenProcess(timer)
  if
    getProcessIDFromProcessName("MetroExodus.exe") ~= nil
  then
    object_destroy(timer)
    openProcess("MetroExodus.exe")
    getAddressList().getMemoryRecordByDescription("[Stats]").Active=true
    getAddressList().getMemoryRecordByDescription("[Others]").Active=true
  end
end

t=createTimer(timer)
timer_setInterval(t,100)
timer_onTimer(t,OpenProcess)
</LuaScript>
